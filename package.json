{
  "name": "ekchylisma",
  "version": "0.1.0",
  "description": "Web-API-first grounded extraction toolkit.",
  "type": "module",
  "private": true,
  "main": "./dist/esm/src/index.js",
  "types": "./dist/types/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/src/index.d.ts",
      "default": "./dist/esm/src/index.js"
    },
    "./node": {
      "types": "./dist/types/src/node/fs.d.ts",
      "default": "./dist/esm/src/node/fs.js"
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "NOTICE",
    "README.md",
    "SECURITY.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "node -e \"require('node:fs').rmSync('dist', { recursive: true, force: true })\"",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "prepack": "npm run build",
    "test": "node tests/run.ts",
    "test:deno": "deno run --allow-read tests/run.ts",
    "test:bun": "bun run tests/run.ts",
    "test:browser": "node tests/browser/harness.test.ts",
    "test:workers": "node tests/workers/harness.test.ts",
    "test:integration": "node tests/providers/integration.test.ts",
    "bench:run": "node bench/run.ts",
    "bench:score": "node bench/score.ts",
    "bench": "npm run bench:run && npm run bench:score",
    "typecheck": "deno check tests/schema/inference.check.ts",
    "repo-scope-check": "node scripts/repo-scope-check.ts",
    "repo-text-check": "node scripts/repo-text-check.ts",
    "oss-check": "node scripts/oss-check.ts",
    "orphan-check": "node scripts/orphan-check.ts",
    "check": "npm run repo-scope-check && npm run repo-text-check && npm run oss-check && npm test && npm run typecheck && npm run orphan-check"
  },
  "dependencies": {},
  "devDependencies": {
    "miniflare": "^3.20250718.3",
    "typescript": "^5.9.2"
  }
}
