{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "contracts/eval-metrics.schema.json",
  "title": "EvalSuiteResult",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schemaValidRate",
    "quoteInvariantRate",
    "uniqueExtractionStability",
    "variance",
    "breadth",
    "runSummaries"
  ],
  "properties": {
    "schemaValidRate": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "quoteInvariantRate": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "uniqueExtractionStability": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "variance": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "runCount",
        "extractionCounts",
        "min",
        "max",
        "mean",
        "stability",
        "stdDev",
        "caseOutcomeDriftRate"
      ],
      "properties": {
        "runCount": { "type": "integer", "minimum": 1 },
        "extractionCounts": {
          "type": "array",
          "items": { "type": "number" }
        },
        "min": { "type": "number" },
        "max": { "type": "number" },
        "mean": { "type": "number" },
        "stability": { "type": "number" },
        "stdDev": { "type": "number", "minimum": 0 },
        "caseOutcomeDriftRate": { "type": "number", "minimum": 0, "maximum": 1 }
      }
    },
    "breadth": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "datasetCaseCount",
        "uniqueCaseIdCount",
        "providerLabelCount",
        "promptVariantCount",
        "seedCount"
      ],
      "properties": {
        "datasetCaseCount": { "type": "integer", "minimum": 0 },
        "uniqueCaseIdCount": { "type": "integer", "minimum": 0 },
        "providerLabelCount": { "type": "integer", "minimum": 1 },
        "promptVariantCount": { "type": "integer", "minimum": 1 },
        "seedCount": { "type": "integer", "minimum": 1 }
      }
    },
    "runSummaries": {
      "type": "array",
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "runIndex",
          "extractionCount",
          "failures",
          "promptVariant",
          "seedLabel",
          "providerLabel"
        ],
        "properties": {
          "runIndex": { "type": "integer", "minimum": 0 },
          "extractionCount": { "type": "integer", "minimum": 0 },
          "failures": { "type": "integer", "minimum": 0 },
          "promptVariant": { "type": "string", "minLength": 1 },
          "seedLabel": { "type": "string", "minLength": 1 },
          "providerLabel": { "type": "string", "minLength": 1 }
        }
      }
    }
  }
}
